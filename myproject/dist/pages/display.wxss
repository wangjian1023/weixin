
.section {
  display: flex;
  width: 100%;
  justify-content: center;
  align-items: center;
  border: 1px solid gray;
  background-color: white;
  height: 30px;
  font-size: 10px;
  margin: 0px 10px;
  justify-content: space-between;
}

.photo {
  display: flex;
  height: 20px;
  width: 20px;
  align-content: center;
  justify-content: center;
  /* padding: 5px; */
}
.background {
  display: flex;
  width: 100%;
  height: 45px;
  align-items: center;
  justify-content: center;
}

.picture {
  height: 14rem;
  width: 95%;
  margin: 2px 10px;
}
.text {
  display: flex;
  border-bottom: 1px solid gray;
  height: 40px;
  flex-direction: column;
  font-size: 15px;
  justify-content: center;
  align-items: center;
  margin: 0px 10px;
}

.sweetheart {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:20px;
  margin-top:15px;
}
.between {
  display: flex;
  width: 100%;
  justify-content: space-between;
}
.atlas {
  display: flex;
  height: 12rem;
  width: 100%;
  justify-content: center;
  align-items: center;
}

.essay {
  display: flex;
  font-size: 13px;
  width: 100%;
  height: 30px;
  margin: 5px 10px;
  
}

.column {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin: 0px 10px;
}

.pattern {
  display: flex;
  background-color: #FFFF99;
  height: 50px;
  width: 100%;
  margin-top: 10px;
  font-size: 15px;
  justify-content: center;
  align-items: center;

}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent
}
